#include "dog.h"

/**
 *init_dog -  function that initialize a variable
 *@d: pointer to structure
 *@name: pointer to name
 *@age: dog's age
 *@owner: pointer to owner
 *Return: void
 **/
void init_dog(struct dog *d, char *name, float age, char *owner)
{
	if (d)
	{
		(*d).name = name;
		(*d).age = age;
		(*d).owner = owner;
	}

}
